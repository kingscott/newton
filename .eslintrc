{
  "extends": "eslint-config-airbnb",
  "rules": {
    "indent": [ 2, 2 ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "always" ],
    "no-console": [ 0 ],
    "no-loop-func": [ 0 ],
    "new-cap": [ 0 ],
    "no-param-reassign": [ 0 ],
    "func-names": [ 0 ],
    "block-scoped-var": [ 0 ],
    "padded-blocks": [ 0 ],
    "camelcase": [ 0 ],
    "brace-style": [ 0 ],
    "comma-dangle": [ 0 ]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true
  }
}
